{
  "compilerOptions": {
    "incremental":false,
    "target":"es2016",
    "module":"commonjs",
    "rootDir":"./",
    "baseUrl":"./",
    "resolveJsonModule":true,
    "declaration":true,
    "sourceMap":true,
    "outDir":"dist",
    "removeComments":true,
    "allowSyntheticDefaultImports":true,
    "esModuleInterop":true,
    "forceConsistentCasingInFileNames":true,
    "noImplicitAny":true,
    "noUnusedLocals":true,
    "noUnusedParameters":true,
    "noFallthroughCasesInSwitch":true,
    "skipLibCheck":true
 },
  "exclude": ["node_modules", "dist"]
}
